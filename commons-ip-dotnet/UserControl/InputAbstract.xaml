        <DockPanel Name="DockPanel" LastChildFill="True" Margin="10,0,0,0">
            <Label Name="Label" Padding="0,2,0,0" Content="Label" Height="26" VerticalAlignment="Top" DockPanel.Dock="Top"/>
        </DockPanel>

    </Grid>
